      "  background: none;\n",
      "  color: $secondary;\n",
      "  border: 1px solid $secondary;\n",
      "}\n",
      "\n",
      ".button_success {\n",
      "  extends-css: button_base;\n",
      "  background: $success;\n",
      "  color: $textInverted;\n",
      "}\n",
      "\n",
      ".button_warning {\n",
      "  extends-css: button_base;\n",
      "  background: $warning;\n",
      "  color: $textInverted;\n",
      "}\n",
      "\n",
      ".button_danger {\n",
      "  extends-css: button_base;\n",
      "  background: $danger;\n",
      "  color: $textInverted;\n",
      "}\n",
      "\n",
      ".button_ghost {\n",
      "  extends-css: button_base;\n",
      "  background: $bgBody;\n",
      "  color: $primary;\n",
      "}\n"
