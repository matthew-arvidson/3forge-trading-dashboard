      ".text {\n",
      "  border-width: 0px 0px 1px 0px;\n",
      "  border-color: $borderHard;\n",
      "  border-radius: 0px;\n",
      "}\n",
      ".text:hover {\n",
      "  background-color: $bgHeader;\n",
      "}\n",
      ".text::selection {\n",
      "  background-color: $primary;\n",
      "  color: $textInverted;\n",
      "}\n",
      "\n",
      ".button_base {\n",
      "  extends-css: button_base;\n",
      "  border: 0px;\n",
      "  border-radius: 4px;\n",
      "}\n",
      "\n",
      ".button_primary {\n",
      "  extends-css: button_base;\n",
      "  background: $primary;\n",
      "  color: $textInverted;\n",
      "}\n",
      "\n",
      ".button_primary_inverted {\n",
      "  extends-css: button_base;\n",
      "  background: none;\n",
      "  color: $primary;\n",
      "  border: 1px solid $primary;\n",
      "}\n",
      "\n",
      ".button_secondary {\n",
      "  extends-css: button_base;\n",
      "  background: $secondary;\n",
      "  color: $textInverted;\n",
      "}\n",
      "\n",
      ".button_secondary_inverted {\n",
